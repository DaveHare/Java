	public void HannoisTaarn(Stack<Integer> X){

		if(B.isEmpty()){
			B.push(X.peek());//a
//			HannoisTaarn(X.pop());
		}else if(C.isEmpty()){
			C.push(X.peek());//a
//			HannoisTaarn(X.pop());

		}else if(A.isEmpty()){
			A.push(X.peek());//c


		}else if(B.peek()<C.peek()){
			C.push(X.pop());//b

		}else if(C.peek()<B.peek()){
			B.push(X.pop());
		}
		}else if(A.peek()<B.peek()){
			B.push(X.pop());
		}




	}
