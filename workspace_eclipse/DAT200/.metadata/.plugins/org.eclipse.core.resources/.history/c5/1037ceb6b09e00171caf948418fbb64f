	public static Integer to;
	public static Integer tre; 

	public static void main(String[] args){
		A=new Stack<Integer>();
		B=new Stack<Integer>();
		C=new Stack<Integer>();

		en= 1;
		to= 2;
		tre= 3;

		A.push(tre);
		A.push(to);
		A.push(en);

	}

	public void HannoisTaarn1(){
		for(Integer brikker: A){

			if(B.isEmpty()){
				B.push(A.pop());
			}else if(B.peek()<A.peek()&& (C.isEmpty()|| A.peek()<B.peek())){
				C.push(A.pop());
			}


		}
		public void HannoisTaarn(){
			
			
			
		}


