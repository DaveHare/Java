	public static Integer to;
	public static Integer tre; 

	public static void main(String[] args){
		A=new ArrayList<Integer>();
		B=new ArrayList<Integer>();
		C=new ArrayList<Integer>();
		
		en= 1;
		to= 2;
		tre= 3;
		
	}
	
	public void HannoisTaarn(){
		A.add(tre);
		A.add(to);
		A.add(en)
		
	}

